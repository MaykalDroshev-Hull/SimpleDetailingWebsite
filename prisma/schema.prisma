// This is your Prisma schema file,
// To properly update db on PlanetScale if changes are made to the table: npx prisma db push

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Review {
  id         Int    @id @default(autoincrement())
  firstName  String
  lastName   String
  reviewText String
}
